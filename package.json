{
  "name": "@jddf/json-schema-to-jddf",
  "version": "0.1.2",
  "description": "Convert JSON Schema schemas into JSON Data Definition Format",
  "author": "Ulysse Carion <ulysse@segment.com>",
  "license": "MIT",
  "files": [
    "lib"
  ],
  "main": "lib/index.js",
  "bin": {
    "json-schema-to-jddf": "./bin/json-schema-to-jddf"
  },
  "scripts": {
    "build": "tsc",
    "fmt": "prettier --write src/**/*.ts",
    "test": "jest"
  },
  "dependencies": {
    "@jddf/jddf": "^0.1.5"
  },
  "devDependencies": {
    "@types/jest": "^24.0.18",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.3",
    "yargs": "^14.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "rootDir": "src",
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "ts"
    ]
  }
